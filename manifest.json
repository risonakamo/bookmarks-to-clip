{
    "manifest_version":2,
    "name":"bookmarks to clip",
    "short_name":"bmclip",
    "description":"copy links of bookmarks in target bookmark folder to clipboard",
    "version":"1.0.0",
    "homepage_url":"https://github.com/risonakamo/bookmarks-to-clip",

    "permissions":[
        "tabs",
        "bookmarks",
        "chrome://favicon/"
    ],

    "browser_action":{
        "default_popup":"popup/popup-index.html"
    }
}